[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/mnt/data/sample/sample
ExecStart=/mnt/data/sample/sample/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/mnt/data/sample/sample/sample.sock sample.wsgi:application

[Install]
WantedBy=multi-user.target
